        var gaId;
        WebFontConfig = { fontdeck: { id: '55547' } };

        switch (window.location.host) {
            // Development environment
            case 'vagrant.local':
                gaId = 'UA-63203045-1';
                break;
            // UA5 server
            case 'storylines.johnny5.ua5host.com':
                gaId = 'UA-63203045-1';
                break;
            // Staging
            case 'exhibitions-staging.guggenheim.org':
                gaId = 'UA-3465995-1';
                break;
            // Production
            case 'exhibitions.guggenheim.org':
                gaId = 'UA-5676651-2';
                break;
        }

        (function() {
          var wf = document.createElement('script');
          wf.src = ('https:' == document.location.protocol ? 'https' : 'http') +
          '://ajax.googleapis.com/ajax/libs/webfont/1/webfont.js';
          wf.type = 'text/javascript';
          wf.async = 'true';
          var s = document.getElementsByTagName('script')[0];
          s.parentNode.insertBefore(wf, s);
        })();
        </script>
